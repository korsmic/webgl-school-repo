{
  "name": "jws-astro-template",
  "author": "-James- Masayoshi Kojima <james@jws.work> (https://github.com/korsmic/)",
  "homepage": "https://jws.work",
  "type": "module",
  "version": "0.0.5",
  "scripts": {
    "dev": "run-p lint:watch astro:dev",
    "build": "run-s lint clean:dist astro:build minify-html beautify",
    "buildwp": "run-s lint clean:dist astro:buildwp beautify",
    "astro:dev": "astro dev --host 0.0.0.0",
    "start": "astro dev",
    "astro:build": "cross-env IS_WP=false astro build",
    "astro:buildwp": "cross-env IS_WP=true astro build",
    "preview": "astro preview",
    "astro": "astro",
    "format": "prettier --write src/**/*.{ts,tsx,astro}",
    "lint": "eslint \"./src/**/*.{js,jsx,ts,tsx,astro}\" --fix --ignore-path .gitignore",
    "lint:watch": "chokidar \"./src/**/*.{js,jsx,ts,tsx,astro}\" -c \"npm run lint\"",
    "clean": "rimraf dist node_modules",
    "clean:dist": "rimraf dist/*",
    "image:clean": "rimraf public/assets/images",
    "image:node": "node imagemin.mjs",
    "image": "run-s image:clean image:node",
    "beautify": "html-beautify dist/**/*.html",
    "minify-html": "node minify-html.mjs"
  },
  "devDependencies": {
    "@rollup/plugin-virtual": "^3.0.1",
    "@types/node": "^20.4.1",
    "@types/three": "^0.153.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "astro": "^2.8.1",
    "autoprefixer": "^10.4.14",
    "chokidar-cli": "^3.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.44.0",
    "eslint-plugin-astro": "^0.27.2",
    "eslint-plugin-import": "^2.27.5",
    "html-minifier": "^4.0.0",
    "image-size": "^1.0.2",
    "include-media": "^2.0.0",
    "js-beautify": "^1.14.8",
    "lil-gui": "^0.18.2",
    "mkdirp": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-normalize-charset": "^6.0.0",
    "postcss-preset-env": "^9.0.0",
    "postcss-sort-media-queries": "^5.2.0",
    "prettier": "^3.0.0",
    "prettier-plugin-astro": "^0.10.0",
    "recursive-readdir": "^2.2.3",
    "rimraf": "^3.0.2",
    "rollup-plugin-glslify": "^1.3.1",
    "sass": "^1.63.6",
    "sharp": "^0.32.2",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "vite-plugin-glsl": "^1.1.2",
    "vite-plugin-glslify": "^2.0.2"
  },
  "dependencies": {
    "astro-relative-links": "^0.3.4",
    "gsap": "npm:@gsap/shockingly@^3.12.2",
    "three": "^0.154.0",
    "tweakpane": "^4.0.1"
  }
}
